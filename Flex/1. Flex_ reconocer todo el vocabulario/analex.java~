          }
        case 143: break;
        case 3: 
          { /* ignora los espacios en blanco */
          }
        case 144: break;
        case 11: 
          { System.out.println("Token Bit_Or <" +yytext()+ "> encontrado en linea: " + (yyline+1) + " columna: " + (yycolumn+1));
          }
        case 145: break;
